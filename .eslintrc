{
  "rules": {
    "indent": [
      0,
      2
    ],
    "quotes": [
      2,
      "single"
    ],
    "linebreak-style": [
      2,
      "unix"
    ],
    "no-console": 0,
    "semi": [
      2,
      "always"
    ]
  },
  "env": {
    "es6": true,
    "node": true,
    "browser": true
  },
  "extends": "eslint:recommended",
  "ecmaFeatures": {
    "jsx": true,
    "experimentalObjectRestSpread": true
  },
  "globals": {
    "describe": true,
    "expect": true,
    "it": true,
    "jest": true
  },
  "parser": "babel-eslint",
  "plugins": [
    "react"
  ]
}